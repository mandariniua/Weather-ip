{
  "title": "IpWeather",
  "description": "elastic.io",
  "docsUrl": "",
  "credentials": {
    "fields": {
      "apiKey": {
        "label": "API key",
        "required": true,
        "viewClass": "TextFieldView"
      }
    }
  },
  "triggers": {
    "ipWeather": {
      "main": "./Backend/ip.js",
      "type": "polling",
      "title": "Show weather",
      "fields": {
        "weatherInfo": {
          "ip": "ip",
          "required": true,
          "viewClass": "SelectView",
          "model": {
            "weather": "Weather"
          }
        },
        "metadata": {
          "out": {}
      }

    }
  }
}